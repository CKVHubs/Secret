local Section = Tab3:AddSection({"Hatcherü•ö"})
local Paragraph = Tab3:AddParagraph({"‚ùåNOT THERE AUTO DELETE‚ùå"})

local eggModelsFolder = game:GetService("ReplicatedStorage").Asserts.EggModels
local eggModelNames = {}

for _, model in ipairs(eggModelsFolder:GetChildren()) do
  table.insert(eggModelNames, '' .. model.Name .. '')
end

local Dropdown = Tab3:AddDropdown({
  Name = "ü•öAuto Hatchingü•ö",
  Description = "Select the <font color='rgb(88, 101, 242)'>Number</font>",
  Options = {"Choose Egg", unpack(eggModelNames)},
  Default = "Choose Egg",
  Flag = "dropdown teste",
  Callback = function(Value)
    if Value ~= "Choose Egg" then
      getfenv().Hatch = true
      while getfenv().Hatch do
        game:GetService("ReplicatedStorage").Events.Pets.Re_Hatch:FireServer("Hatch", Value, {})
        wait(1)
      end
    else
      getfenv().Hatch = false
    end
  end
})

Tab3:AddButton({"Stop", function()
  getfenv().Hatch = false
end})

local Section = Tab3:AddSection({"Trainingüëä"})
local Paragraph = Tab3:AddParagraph({"Execute Script Again to refresh"})
local TrainingFolder1 = game.workspace.Worlds[1]["Êú®Ê°©"]
local TrainingFolder2 = game.workspace.Worlds[2]["Êú®Ê°©"]
local TrainingFolder3 = game.workspace.Worlds[3]["Êú®Ê°©"]
local TrainingFolder4 = game.workspace.Worlds[4]["Êú®Ê°©"]
local TrainingFolder5 = game.workspace.Worlds[5]["Êú®Ê°©"]
local TrainingFolder6 = game.workspace.Worlds[6]["Êú®Ê°©"]
local TrainingFolder7 = game.workspace.Worlds[7]["Êú®Ê°©"]
local TrainingFolder8 = game.workspace.Worlds[8]["Êú®Ê°©"]
local TrainingName = {}

for _, model in ipairs(TrainingFolder1:GetChildren()) do
  table.insert(TrainingName, '' .. model.Name .. '')
end

for _, model in ipairs(TrainingFolder2:GetChildren()) do
  table.insert(TrainingName, '' .. model.Name .. '')
end

local Dropdown = Tab3:AddDropdown({
  Name = "üëäAuto Trainingüëä",
  Description = "Select the <font color='rgb(88, 101, 242)'>Number</font>",
  Options = {"Choose NPC", unpack(TrainingName)},
  Default = "Choose NPC",
  Flag = "dropdown teste",
  Callback = function(Value)
    if Value ~= "Choose NPC" then
      getfenv().NPC = true
      while getfenv().NPC do
        game:GetService("ReplicatedStorage").Events.Game.Re_TrainPower:FireServer((Value))
        wait(1)
      end
    else
      getfenv().NPC = false
    end
  end
})

Tab3:AddButton({"Stop", function()
  getfenv().NPC = false
end})

local Section = Tab4:AddSection({"StatsüìÉ"})
local Paragraph = Tab4:AddParagraph({"‚ùóDON'T FORGOT USE ANTIAFK‚ùó"})

local Button = Tab4:AddButton({"Infinite Jump", function()
        local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
if InfiniteJumpEnabled then
game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
end
end)
end})

Tab4:AddSlider({
  Name = "JumpPower",
  Min = 50,
  Max = 200,
  Increase = 1,
  Default = 50,
  Callback = function(Value)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")

    humanoid.JumpPower = (Value)
  end
})

Tab4:AddSlider({
  Name = "WalkSpeed Fast",
  Min = 16,
  Max = 125,
  Increase = 1,
  Default = 16,
  Callback = function(Value)
    local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

humanoid.WalkSpeed = (Value)

  end
})

local Button = Tab4:AddButton({"Dont get kick after 20 minutes(antiafk)", function()
  for i,v in pairs(getconnections(game:GetService("Players").LocalPlayer.Idled)) do
    v:Disable()
end
end})


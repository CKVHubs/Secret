local Button = Tab4:AddButton({"Remove Textures", function()
  local Dialog = Window:Dialog({
    Title = "Warning",
    Text = "Are you sure want remove textures :)?",
    Options = {
      {"Confirm", function()
        for i,v in next, (workspace:GetDescendants()) do
 if v:IsA("Part") then v.Material = Enum.Material.SmoothPlastic
 end
 end
      end},
     
      {"Cancel", function()
        
      end}
    }
  })
end})

local Button = Tab4:AddButton({"Remove Players", function()
  local Dialog = Window:Dialog({
    Title = "Warning",
    Text = "Are you sure want hide Players :)?",
    Options = {
      {"Confirm", function()
        -- Get all players in the game
local players = game:GetService("Players"):GetPlayers()

-- Get the player's character
local myCharacter = game.Players.LocalPlayer.Character

-- Loop through all players and delete their characters
for _, player in ipairs(players) do
    if player.Character ~= myCharacter then
        player.Character:Destroy()
    end
end
      end},
     
      {"Cancel", function()
        
      end}
    }
  })
end})

local Button = Tab4:AddButton({"Hide AnyThing", function()
  local Dialog = Window:Dialog({
    Title = "Warning",
    Text = "Are you sure want hide anything :)?",
    Options = {
      {"Confirm", function()
        for _, part in pairs(workspace:GetDescendants()) do
    if part:IsA("BasePart") then
        part.Transparency = 1
    end
end
      end},
     
     {"Unhide", function()
        for _, part in pairs(workspace:GetDescendants()) do
    if part:IsA("BasePart") then
        part.Transparency = 0
    end
end
      end},
     
      {"Cancel", function()
        
      end}
    }
  })
end})

Tab3:AddSlider({
  Name = "WalkSpeed Fast",
  Min = 16,
  Max = 125,
  Increase = 1,
  Default = 16,
  Callback = function(Value)
    local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

humanoid.WalkSpeed = (Value)

  end
})

local Button = Tab3:AddButton({"Infinite Jump", function()
        local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
if InfiniteJumpEnabled then
game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
end
end)
end})

local Button = Tab3:AddButton({"Dont get kick after 20 minutes(antiafk)", function()
  for i,v in pairs(getconnections(game:GetService("Players").LocalPlayer.Idled)) do
    v:Disable()
end
end})


Toggle1:Callback(function(state1)
  Toggle1:Set(false)
  getfenv().autotest1 = (state1 and true or false)
    while getfenv().autotest1 do
Toggle3:Set(true)
Toggle4:Set(false)
Toggle5:Set(false)
Toggle6:Set(false)
Toggle7:Set(false)
Toggle8:Set(false)
  local fightNpcsFolder = game.workspace:FindFirstChild("FightNpcs")

if fightNpcsFolder and #fightNpcsFolder:GetChildren() == 0 then -- Check if FightNpcs folder exists and is empty
wait(3)
Toggle2:Set(false)
wait(1)
  game:GetService("ReplicatedStorage").Events.Fight.Re_ChallengeStart:FireServer(1) 
wait(3)
  game:GetService("ReplicatedStorage").Events.Fight.Re_ChallengeStart:FireServer(7)
wait(0.5)
Toggle2:Set(true)
end
wait(1)
end
end)

Toggle2:Callback(function(state2)
  Toggle2:Set(false)
  getfenv().autotest2 = (state2 and true or false)
    while getfenv().autotest2 do
    Toggle3:Set(true)
Toggle4:Set(false)
Toggle5:Set(false)
Toggle6:Set(false)
Toggle7:Set(false)
    local npc = game.Workspace.FightNpcs:GetChildren()[1]
        if npc then
            local npcName = npc.Name

            local args = {
                [1] = npcName, 
                [2] = 1,
            }

            game:GetService("ReplicatedStorage").Events.Fight.Re_TakeDamage:FireServer(unpack(args))
  end
wait(1)
end
end)

Toggle3:Callback(function(state3)
  Toggle3:Set(false)
  getfenv().autotest3 = (state3 and true or false)
    while getfenv().autotest3 do
    
  local fightNpcsFolder = workspace:FindFirstChild("FightNpcs")

        if fightNpcsFolder then
            for _, child in pairs(fightNpcsFolder:GetChildren()) do
                local statusObject = child:FindFirstChild("AttackTime")

                if statusObject then
                    statusObject.Value = 900000000000 -- Set the AttackTime value to 900000
                end
            end
        end
wait(1)
end
end)


